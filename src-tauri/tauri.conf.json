{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "cotwkit",
  "version": "0.1.0",
  "identifier": "com.cotwkit.triabin",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "狩猎工具包",
        "width": 1280,
        "height": 720,
        "center": true,
        "decorations": false,
        "fullscreen": false,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "wix": {
        "language": ["zh-CN", "en-US"]
      }
    }
  }
}
